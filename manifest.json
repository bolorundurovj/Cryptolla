{
  "manifest_version": 2,
  "name": "Password Gen",
  "description": "A Simple and Easy to Use Secure Password Generator",
  "version": "1.1.0",
  "icons": {
    "128": "icons8_password_160px.png",
    "32": "icons8_password_32px.png",
    "60": "icons8_password_60px.png",
    "80": "icons8_password_80px.png"
  },
  "browser_action": {
    "default_icon": "icons8_password_60px.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
